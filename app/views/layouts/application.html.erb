<!DOCTYPE html>
<html>
<head>
  <title>SocialRecipies</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div>
    <% unless user_signed_in? %>
      <%= button_to "Login User", new_user_session_path %>
    <% else %>
      <%= button_to "Logout User", destroy_user_session_path, :method => :delete  %>
    <% end %>
  </div>

<%= yield %>
  <div>
    <% unless admin_signed_in? %>
    <%= button_to "Login Admin", new_admin_session_path %>
    <% else %>
    <%= button_to "Logout", destroy_admin_session_path, :method => :delete  %>
    <% end %>
  </div>

</body>
</html>
